{
    "name": "pbxhq/kazoo",
    "description": "Kazoo Wrapper for easy Kazoo API calls",
    "type": "library",
    "require": {
        "laravel/framework": "^8.0",
        "guzzlehttp/guzzle": "^7.0.1"
    },
    "authors": [
        {
            "name": "Aaron",
            "email": "aaron@pbxhq.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Pbxhq\\Kazoo\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Pbxhq\\Kazoo\\KazooServiceProvider"
            ],
            "aliases": {
                "Kazoo": "Pbxhq\\Kazoo\\KazooController"
            }
        }
    }
}
